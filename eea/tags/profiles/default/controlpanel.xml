<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="eea">

    <configlet
        title="EEA Tags"
        action_id="eea.tags"
        appId="eea.tags"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@eeatags-controlpanel"
        icon_expr="string:${portal_url}/maintenance_icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
